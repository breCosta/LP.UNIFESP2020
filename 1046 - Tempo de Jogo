#include <stdio.h>
 
int main() {
  int i, f,total;
  scanf("%d %d", &i, &f);
  if (i<f){
    total=f-i;
    printf("O JOGO DUROU %d HORA(S)\n", total);
  }
  if(f<i){
    total=f+(24-i);
    printf("O JOGO DUROU %d HORA(S)\n", total);
  }
    if (i==f){
    printf("O JOGO DUROU 24 HORA(S)\n");
  }
 
    return 0;
}
