#include <stdio.h>
 
int main() {
 
 int a, b, c;
  scanf("%d %d %d", &a, &b, &c);
  if (a>b && b>c){
    printf("%d\n%d\n%d\n\n", c, b, a);
  }
  if (a>b && c>b && a>c){
    printf("%d\n%d\n%d\n\n", b, c, a);
  }
  if (b>a && a>c){
    printf("%d\n%d\n%d\n\n", c, a, b);
 }
  if (b>a && c>a && b>c){
   printf("%d\n%d\n%d\n\n", a, c, b);
 }
  if (c>a && b>a && c>b){
  printf("%d\n%d\n%d\n\n", a, b, c);
  }
  if (c>a && a>b){
  printf("%d\n%d\n%d\n\n", b, a, c);  
  }
printf("%d\n%d\n%d\n", a, b, c);
    return 0;
}
