#include <stdio.h>
 
int main() {

  double A, B, C, delta, raiz1, raiz2;
  scanf("%lf %lf %lf", &A, &B, &C);
  delta=pow(B,2) -4*A*C;
  raiz1= (-B + sqrt(delta))/(2*A);
  raiz2= (-B - sqrt(delta))/(2*A);
  if (delta >= 0 && A!=0) {
    printf("R1 = %.5lf\n", raiz1);
    printf("R2 = %.5lf\n", raiz2);
 } else {
   printf("Impossivel calcular\n");
 }

 
    return 0;
}
